{
  "name": "slinky",
  "version": "1.0.0",
  "description": "A GUI for npm link management",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "verify": "npm run verify:js --silent",
    "verify:js": "eslint -c config/verify/.eslintrc \"js/**/*.js\" && echo verify:js success",
    "verify:js:watch": "chokidar 'js/**/*.js' -c 'npm run verify:js' --initial --silent",
    "verify:watch": "npm run verify:js:watch --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodleviton/slinky.git"
  },
  "keywords": [
    "Electron",
    "NPM"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/rodleviton/slinky/issues"
  },
  "devDependencies": {
    "babel-preset-es2016-node5": "1.1.2",
    "electron": "1.3.4",
    "electron-compilers": "3.1.6"
  },
  "dependencies": {
    "chokidar-cli": "1.2.0",
    "electron-compile": "3.3.5",
    "eslint": "1.10.3",
    "eslint-config-defaults": "8.0.2",
    "lodash": "4.15.0",
    "menubar": "5.1.0",
    "npm": "3.10.7",
    "slide": "^1.1.6"
  }
}
