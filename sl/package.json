{
  "name": "slinky",
  "version": "1.0.0",
  "description": "A GUI for npm link management",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "verify": "npm run verify:js && npm run test",
    "verify:js": "eslint -c config/verify/.eslintrc 'js/**/*.js' && echo 'verify:js success'",
    "test:watch": "chokidar 'js/**/*.js' 'test/**/*.js' -c 'npm run test' --initial --silent",
    "test": "mocha --compilers js:babel-core/register -R spec ./test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReactivePixels/slinky.git"
  },
  "keywords": [
    "Electron",
    "NPM"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ReactivePixels/slinky/issues"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016-node5": "1.1.2",
    "electron": "1.3.4",
    "electron-compilers": "3.1.6",
    "electron-mocha": "^3.1.1",
    "electron-prebuilt": "^1.4.1",
    "mocha": "^3.1.0",
    "spectron": "^3.4.0"
  },
  "dependencies": {
    "chokidar-cli": "1.2.0",
    "electron-compile": "3.3.5",
    "eslint": "1.10.3",
    "eslint-config-defaults": "8.0.2",
    "lodash": "4.15.0",
    "menubar": "5.1.0",
    "npm": "3.10.7",
    "slide": "^1.1.6"
  }
}
